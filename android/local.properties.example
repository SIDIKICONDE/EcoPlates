# Android Local Configuration Template
# Copy this file to local.properties and fill in the actual values
# DO NOT commit local.properties to version control

# Flutter SDK path (auto-generated by Flutter, don't modify)
# flutter.sdk=C:\\flutter

# Google Maps API Key for Android
# Get your key from: https://console.cloud.google.com/google/maps-apis
# Ensure you enable "Maps SDK for Android" and properly restrict the key
googleMapsApiKey=your_google_maps_api_key_here

# Signing configuration (for release builds)
# storeFile=../path/to/your/keystore.jks
# storePassword=your_store_password
# keyPassword=your_key_password
# keyAlias=your_key_alias